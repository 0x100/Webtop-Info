{
  "manifest_version": 2,  
  "name": "Webtob Info",
  "version": "1.0",
  
  "permissions": [
    "webRequest",
    "<all_urls>"
  ],  
  "background": {
        "scripts": ["background.js"]
  },
  "minimum_chrome_version": "17",
  "browser_action": {
    "default_icon": "images/48.png",
    "default_popup": "popup.html"
  },
  "icons": {
        "32": "images/32.png",
        "48": "images/48.png",
        "64": "images/64.png",
        "128": "images/128.png"
   }
}